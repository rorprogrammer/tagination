#tab{
	margin:auto;
	width: 1155px;
	min-height: 100%;
	padding-top: 40px;
}

#tab_content{
	min-height: 100%;
	background: #90ABBE;
	border: 2px solid #eee;
	text-align: center; 
	min-width: 100%;
}

#header {
  /* the tab div needs to match the page background */
  font-size:100%;
 
}
#header ul {
  padding: 0px;
  margin: 0px;
  list-style: none;
  height: 25px;
}

#header li {
  float:left;
  background: #ccc;
  border-color: #425C70;
  border-style: solid;
  border-width: 2px 2px 0;
  margin: 0px 5px 0px 0px;
}

#header a{
  float: left; /* one half of IE5 hack */
  display: block;
  background: #ccc;
  padding: 2px;
  width: 8em;
  text-align: center;
  text-decoration: none;
  font-weight: bold;
  color:#fff;
}
#header a:hover{
  color: black;
  background-color: #E3EDEC;
}
#header #current {
  background: #90ABBE;
  border-color: #eee;
}
#header #current a {
  background:#90ABBE repeat-y;
  color:#222;
  border-bottom: 2px solid #90ABBE;
}
/* Commented Backslash Hack
   hides rule from IE5-Mac \*/
#header a {float:none;}
/* End IE5-Mac hack */

/* IE6 hack for padding around tabs */
* html #header a {padding: 0px 0px 5px 15px;}

fieldset{padding: 10px; margin: 10px;}

.searched_tags img{ border: none; margin-left: -2px; margin-right: 2px;}

p.cloud{padding: 0 30px;line-height: 25px;}

p.cloud a:hover{background-color: #D78807;}

.tag{padding: 0 8px 0 0;}

#result_lists{
	margin: auto;
	margin-bottom: 45px;
	width: 1155px;
	min-height: 100%;
	padding: 20px 0px;
}
.table_row{
	height: auto;
	min-height: 265px;
	width: 1155px;
	text-align: center;
}

.list_header{position: relative;}

.list_header ul{
	background: #C7DC67;
	border-bottom: 1px solid #888;
	position: relative;
	list-style: none;
	height: 20px;
	padding: 4px 8px;
	margin: 0px;
}
li.list_header_title{ float:left; padding: 2px 0px;}
li.list_header_results{ position: relative; float: left; left: 38%;}
li.list_header_pages{float: right; padding: 2px 0px;}
li.list_header_pages a{
	text-decoration:none;
	border: solid 1px #aaa;
	padding: 0 4px 0 4px;
	color: #eb7d26;
}

.list_navigation{
	font-size: 14px;
	padding: 0px;
}

.list_navigation ul{
	position: relative;
	list-style: none;
	background: #eee;
	border-bottom: 2px solid #ddd;
	height: 20px;
	padding: 4px 8px;
	margin: 0px;
}

/* css for resource adn resourcelist tables
 * first the resource table*/

div.tr { display:table-row; }
div.td { 
	display:table-cell; 	
	padding: 3px; 
	vertical-align: top;
}
/* second the resourcelist table*/

div#res_table { 
	display:table; 
	border-collapse:collapse; 
	margin: auto;
	width: 138px;
}
div.tr_image_photo{ 
	height: 128px; 
	display:table-row;
}
div.tr_image_photo img{
	border: 1px solid #aaa;
	padding: 4px;
	background-color: #eee;
}
div.tr_image_video{
	height: 128px; 
	display:table-row;
}
div.tr_image_video img{
	border: 1px solid #ccc;
	background-color: #eee;
	width: 120px;
	height: 90px;
}
div.tr_image_bookmark{
	height: 128px; 
	display:table-row;
}
div.tr_image_bookmark img{
	border: 1px solid #aaa;
	padding: 2px;
	background-color: #eee;
	width: 120px;
	height: 90px;
}

div.tr_title{
	font-size: 14px; font-weight: bold;
	height: 40px; display:table-row;
}
div.tr_date{
	display:table-row;
	font-size: 12px;
	height: 22px;
}
div.tr_tags{
	display:table-row;
	font-size: 11px; 
	background-color: #eee; 
}
div.td_res{
	display:table-cell; 
	vertical-align: middle;
}
p.img{margin: 0px;}
div.tr_tags .td_res{ height: 65px;text-align: center;}

.list_tag a{vertical-align:middle; color:#425C70;}
.list_tag a:hover{background-color: #D78807;}

#preview{
	background: #333;
	position:absolute;
	padding:5px;
	max-height: 250px;
	max-width: 250px;
	display:none;
	color:#fff;
}
#preview img{
	max-height: 220px;
	max-width: 250px;
}

span.page_link a{
	color: #425C70;
	text-decoration: none;
	padding: 0px 3px;
	margin: 0px 2px;
	border: solid 1px #bbb;
	padding: 0px 2px;
}
span.page_link_current {
	color: #425C70;
	text-decoration: none;
	background-color: #ccc;
	padding: 0px 2px;
	margin: 0px 2px;
	border: solid 2px #aaa;
}
span a:hover{
	background-color: #ccc;
}

.four_list{
	float:inherit;
	margin: auto;
	margin-bottom: 12px;
	margin-right: -8px;
	position: relative;
	border:solid #f9f9f9; 
	border-width:0px 1px 1px 0px; 
}

